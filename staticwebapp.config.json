{
  "routes": [
    {
        "route": "/*",
        "redirect": "https://platformui.com",
        "statusCode": 301
    }
  ],
  "responseOverrides": {
    "403": {
      "rewrite": "/404/index.html"
    },
    "404": {
      "rewrite": "/404.html"
    }
  },
  "mimeTypes": {
    ".json": "text/json"
  }
}